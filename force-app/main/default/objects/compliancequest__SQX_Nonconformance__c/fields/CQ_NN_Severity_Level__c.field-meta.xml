<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CQ_NN_Severity_Level__c</fullName>
    <externalId>false</externalId>
    <formula>CASE(
    MAX(
        CASE(TEXT(CQ_NN_HSE_Risk_Severity__c), &quot;Negligible&quot;, 1, &quot;Light&quot;, 2, &quot;Serious&quot;, 3, &quot;Major&quot;, 4, &quot;Catastrophic&quot;, 5, 0),
        CASE(TEXT(CQ_NN_Customer_Impact__c), &quot;Negligible&quot;, 1, &quot;Light&quot;, 2, &quot;Serious&quot;, 3, &quot;Major&quot;, 4, &quot;Catastrophic&quot;, 5, 0),
        CASE(TEXT(CQ_NN_Internal_Fiscal_Liability__c), &quot;Negligible&quot;, 1, &quot;Light&quot;, 2, &quot;Serious&quot;, 3, &quot;Major&quot;, 4, &quot;Catastrophic&quot;, 5, 0),
        CASE(TEXT(CQ_NN_Product_Operational_Risk__c), &quot;Negligible&quot;, 1, &quot;Light&quot;, 2, &quot;Serious&quot;, 3, &quot;Major&quot;, 4, &quot;Catastrophic&quot;, 5, 0),
        CASE(TEXT(CQ_NN_Unique_or_Systemic__c), &quot;Negligible&quot;, 1, &quot;Light&quot;, 2, &quot;Serious&quot;, 3, &quot;Major&quot;, 4, &quot;Catastrophic&quot;, 5, 0)
    ),
    1, &quot;Negligible&quot;,
    2, &quot;Light&quot;,
    3, &quot;Serious&quot;,
    4, &quot;Major&quot;,
    5, &quot;Catastrophic&quot;,
    &quot;Unknown&quot;
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Severity Level</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
